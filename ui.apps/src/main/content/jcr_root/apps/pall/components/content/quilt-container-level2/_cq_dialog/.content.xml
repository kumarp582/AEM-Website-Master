                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="/libs/granite/ui/components/foundation/form/radio"
                                        id="12b"
                                        text="12 Tiles - 1 (2X2), 2 (1X2), 9 (1X1)"
                                        value="12b Tiles - 1 (2X2), 2 (1X2), 9 (1X1)"/>
