                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="/libs/granite/ui/components/foundation/form/radio"
                                        id="12a"
                                        text="12 Tiles - 1 (2X2), 1 (2X1), 1 (1X2), 9 (1X1)"
